    <script>
function changeImage(img) {
            let mainImage = document.getElementById("ima");
            mainImage.src =img.src;
        }
        function startCountdown(durationInMinutes) {
    let timeleft = localStorage.getItem("timeleft") ? parseInt(localStorage.getItem("timeleft")) : durationInMinutes * 60;
    const timerElement = document.getElementById("timer");
const discount=document.querySelector(".p strong")
const oldelement=document.querySelector(".para strong")
const discountelement=document.querySelector(".para span")
const offertext=document.querySelector(".par")
const button=document.querySelectorAll(".buy")

    function updateTimer() {
        let minutes = Math.floor(timeleft / 60);
        let seconds = timeleft % 60;
        timerElement.textContent = (minutes < 10 ? "0" : "") + minutes + ":" + (seconds < 10 ? "0" : "") + seconds;
localStorage.setItem("timeleft",timeleft)
        if (timeleft > 0) {
            timeleft--;
            setTimeout(updateTimer, 1000); // Update every second

        } else {
            expiraryoffer()

    }
    
    
}
function expiraryoffer(){
        timerElement.textContent="offer expired"
        
            offertext.style.color="red"

discount.textContent="500"
discount.style.textDecoration="line-through"

oldelement.textContent="no discount"
oldelement.style.color="red"
discountelement.style.textDecoration="none"
localStorage.setItem("offerexpired","true")


button.forEach(bttn =>{
    bttn.style.color="red"
    bttn.disabled=true
    bttn.textContent="offer end"
})
    }
    if(localStorage.getItem("offerexpired") === "true"){
        expiraryoffer()
    }
    else{
updateTimer();

    }

}
// Start the countdown for 1 minute (change the number to increase duration)
startCountdown(1);

